{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@clawtab/shared": ["../shared/src"],
      "@clawtab/shared/*": ["../shared/src/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "../shared/src/**/*.ts",
    "../shared/src/**/*.tsx"
  ]
}
