{
  "expo": {
    "name": "ClawTab",
    "slug": "clawtab",
    "version": "1.0.0",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 30000,
      "url": "https://ota.clawtab.cc/manifest"
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "clawtab",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "cc.clawtab",
      "entitlements": {
        "keychain-access-groups": ["$(AppIdentifierPrefix)cc.clawtab"],
        "aps-environment": "production"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "edgeToEdgeEnabled": true,
      "package": "cc.clawtab"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#7986cb"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.186596496380-9rdm6vtappp444abiuftee5fde9eh56j"
        }
      ],
      [
        "expo-nse-plugin",
        {
          "mode": "production",
          "nse": {
            "sourceFiles": "./nse/NotificationService.swift",
            "bundleName": "ClawTabNotificationService"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "ClawTabNotificationService",
                  "bundleIdentifier": "cc.clawtab.ClawTabNotificationService",
                  "entitlements": {
                    "aps-environment": "production"
                  }
                }
              ]
            }
          }
        }
      }
    }
  }
}
